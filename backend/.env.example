# Environment
DEBUG=true
# ENV can be: development or production
ENV=development

# Database URLs
# Production Database
DATABASE_URL_PRODUCTION=postgresql+asyncpg://your-production-connection-string

# Development Database
DATABASE_URL_DEVELOPMENT=postgresql+asyncpg://postgres:postgres@localhost:5432/rfp_analyzer

# GCP Settings
GCP_PROJECT_ID=your-project-id
GCP_LOCATION=us-central1
GCS_BUCKET=your-bucket-name

# Gemini
GEMINI_MODEL=gemini-3-pro-preview

# MCP Talent Search Server
MCP_TALENT_URL=http://localhost:8083

# JWT
JWT_SECRET_KEY=change-this-in-production-use-a-long-random-string

# CORS (comma separated)
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]
